{
    "name": "carbon/primarycontent",
    "type": "neos-carbon",
    "description": "Extended implementation from PrimaryContent",
    "license": "MIT",
    "keywords": [
        "flow",
        "neos",
        "fusion",
        "helper",
        "content",
        "carbon"
    ],
    "require": {
        "neos/neos": "~5.3 || ~7.0 || ~8.0",
        "carbon/eel": "^1.2 || ^2.0"
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "hello@uhlmann.pro",
            "homepage": "https://www.uhlmann.pro",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".github",
            ".gitignore",
            "CODE_OF_CONDUCT.md"
        ]
    },
    "autoload": {
        "psr-4": {
            "Carbon\\PrimaryContent\\": "Classes"
        }
    },
    "extra": {
        "installer-name": "Carbon.PrimaryContent",
        "neos": {
            "package-key": "Carbon.PrimaryContent"
        }
    }
